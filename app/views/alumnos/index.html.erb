<h1>Listing alumnos</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Numero de cuenta</th>
    <th>Facultad</th>
    <th>Semestre</th>
    <th>Grupo</th>
    <th>Carrera</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @alumnos.each do |alumno| %>
  <tr>
    <td><%= alumno.nombre %></td>
    <td><%= alumno.cuenta%></td>
    <td><%= alumno.facultad %></td>
    <td><%= alumno.semestre %></td>
    <td><%= alumno.grupo %></td>
    <td><%= alumno.carrera %></td>

    <td><%= link_to 'Edit', edit_alumno_path(alumno) %></td>
    <td><%= link_to 'Destroy', alumno, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Alumno', new_alumno_path %>
